<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#373433">


    <ImageView
        android:id="@+id/imageViewMenuAmarelo3"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/btbg2" />

    <Button
        android:id="@+id/button_hamburguer_slide_menu2"
        android:layout_width="35dp"
        android:layout_height="25dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="70dp"
        android:layout_marginRight="70dp"
        android:background="@drawable/hamburguer_slide_menu"
        app:layout_constraintBottom_toTopOf="@+id/imageViewMenuAmarelo3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.628"
        app:layout_constraintStart_toEndOf="@+id/imageViewMenuAmarelo3"
        app:layout_constraintTop_toBottomOf="@+id/imageViewMenuAmarelo3"
        app:layout_constraintVertical_bias="0.494" />

    <Button
        android:id="@+id/button_back_arrow_black3"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="360dp"
        android:layout_marginRight="360dp"
        android:background="@drawable/back_menu_black_arrow"
        app:layout_constraintBottom_toTopOf="@+id/imageViewMenuAmarelo3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageViewMenuAmarelo3"
        app:layout_constraintTop_toBottomOf="@+id/imageViewMenuAmarelo3"
        app:layout_constraintVertical_bias="0.542" />

    <ImageView
        android:id="@+id/btn_Marcar_Viagem"
        android:layout_width="330dp"
        android:layout_height="80dp"
        android:layout_marginBottom="296dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/marcar_viagem" />

    <ImageView
        android:id="@+id/bt_Viagens_Marcadas"
        android:layout_width="330dp"
        android:layout_height="80dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Marcar_Viagem"
        app:srcCompat="@drawable/viagens_marcadas" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="43dp"
        android:layout_marginLeft="43dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="197dp"
        android:layout_marginRight="197dp"
        android:text="O que pretende fazer?"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_Marcar_Viagem"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/user_outer"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/btn_Viagens_Efetuadas"
        android:layout_width="330dp"
        android:layout_height="80dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bt_Viagens_Marcadas"
        app:srcCompat="@drawable/clock" />

    <ImageView
        android:id="@+id/imageView23"
        android:layout_width="330dp"
        android:layout_height="80dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_Viagens_Efetuadas"
        app:srcCompat="@drawable/pesquisar_user" />

    <TextView
        android:id="@+id/txt_Marcar_Viagem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Marcar Viagem"
        android:textColor="#FFFFFF"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/btn_Marcar_Viagem"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toEndOf="@+id/btn_Marcar_Viagem"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/txt_Viagens_Marcadas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Viagens Marcadas"
        android:textColor="#FFFFFF"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/bt_Viagens_Marcadas"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toEndOf="@+id/bt_Viagens_Marcadas"
        app:layout_constraintTop_toBottomOf="@+id/btn_Marcar_Viagem" />

    <TextView
        android:id="@+id/txt_Viagens_Efetuadas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="@string/ver_viagens_efetuadas"
        android:textColor="#FFFFFF"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/btn_Viagens_Efetuadas"
        app:layout_constraintHorizontal_bias="0.439"
        app:layout_constraintStart_toEndOf="@+id/btn_Viagens_Efetuadas"
        app:layout_constraintTop_toBottomOf="@+id/txt_Viagens_Marcadas" />

    <TextView
        android:id="@+id/txt_Pesquisar_utilizador"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="Pesquisar Utilizador"
        android:textColor="#FFFFFF"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/imageView23"
        app:layout_constraintHorizontal_bias="0.461"
        app:layout_constraintStart_toEndOf="@+id/imageView23"
        app:layout_constraintTop_toBottomOf="@+id/btn_Viagens_Efetuadas" />

    <ImageView
        android:id="@+id/user_outer"
        android:layout_width="273dp"
        android:layout_height="216dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/caixa_blck"
        tools:layout_editor_absoluteY="85dp" />

    <ImageView
        android:id="@+id/user_Inner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ic_launcher_foreground"
        tools:layout_editor_absoluteX="153dp"
        tools:layout_editor_absoluteY="107dp" />

    <TextView
        android:id="@+id/user_Name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="User_Nome"
        android:textColor="#FCD100"
        android:textSize="18sp"
        app:layout_constraintEnd_toStartOf="@+id/user_outer"
        app:layout_constraintStart_toEndOf="@+id/user_outer"
        tools:layout_editor_absoluteY="215dp" />

    <TextView
        android:id="@+id/user_localidade"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="User_Localidade"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/user_outer"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toEndOf="@+id/user_outer"
        tools:layout_editor_absoluteY="253dp" />


</androidx.constraintlayout.widget.ConstraintLayout>